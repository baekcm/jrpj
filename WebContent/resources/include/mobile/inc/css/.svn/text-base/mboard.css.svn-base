/*table type02*/
.bd00view {
	table-layout: fixed;
	width: 100%;
	background:url(/resources/include/mobile/image/bg_pagehr.png) 0 0 no-repeat;
	padding-top:2px;
	border-bottom: 1px solid #84c7f3;
	border-collapse: separate;
	border-spacing: 0;
}
.bd00view th {
	padding: 2px 5px;
	border-bottom: 1px solid #ccc;
	background: #f5f5f5
}
.bd00view td {
	padding: 2px 5px;
	border-bottom: 1px solid #ccc;
}
.bd00view td.point_blue04 a {
	color: #3468d4;
	text-decoration: underline
}
.bd00view td.contents {
	padding: 10px
}
.paging {
	text-align: center;
	margin: 0 auto;
	overflow: hidden;
	position: relative;
	height: 24px;
	width: 100%;
	margin-top: 15px;
}
.paging .pagePrev {
	position: absolute;
	top: 0;
	left: 0;
}
.paging .pageNext {
	position: absolute;
	top: 0;
	right: 0;
}
.paging .page a {
	background: url(/resources/include/mobile/image/board/bg_pageNoOff.png) 0 0 no-repeat;
	display: inline-block;
	overflow: hidden;
	width: 22px;
	height: 20px;
	text-align: center;
	padding-top: 2px;
	margin-left: 5px;
}
.paging .page a:first-child {
	margin: 0
}
.paging .page a.on, .paging .page a:hover, .paging .page a:focus {
	background: url(/resources/include/mobile/image/board/bg_pageNoOn.png) 0 0 no-repeat;
	display: inline-block;
	overflow: hidden;
	width: 22px;
	height: 20px;
	text-align: center;
	padding-top: 2px;
	color: #007ac5;
	text-decoration: underline
}
.btnFarmR {
	margin-top: 15px;
	float: right
} 

/*FAQ용*/
.bd02 {
	/*table-layout: fixed;*/
	border-collapse: separate;
	border-spacing: 0px;
	border-top: 2px solid #45bfdd;
	margin: 10px 0;
}
.bd02 th {
	color: #666;
	height: 31px;
	background: url(../../image/common/bg_underLine.gif) 0 100% repeat-x;
	border-bottom: 1px solid #ededed;
	position:relative
}
.bd02 th.end {
	color: #666;
	height: 31px;
	text-align: left;
	background: url(/image/common/icon_q.gif) 10px 10px no-repeat;
}
.bd02 td {
	height: 31px;
	background: #f3fbff url(../../image/common/bg_underLine.gif) 0 100% repeat-x;
	border-bottom: 1px solid #ededed
}
.bd02 td .answer {
	display: block;
	overflow: hidden;
	width: 20px;
	float: left;
	padding-left: 20px;
	padding-top: 10px;
}
.bd02 .icon { width: 40px; overflow:hidden; height:30px; display: inline-block; position:absolute; top:10px; left:10px}
.bd02 .question { padding-left:40px; display:inline-block; }
.bd02 td .text {
	overflow: hidden;
	display: block;
	padding: 10px;
	text-align: left;
}
.bd02 td.c {
	background: #ebf2f5;
	border-bottom: 1px solid #d3d3d3;
	border-right: 1px solid #d3d3d3;
	text-align: center;
	height: 31px;
}
.bd02 td.re {
	border-right: 0 none;
	height:auto;
}
.bd02 span.listx {
	width: auto;
	overflow: hidden;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-ms-text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}
.bd02 span.listc {
	width: auto;
	overflow: hidden;
	text-overflow: clip;
	-o-text-overflow: clip;
	-ms-text-overflow: clip;
	white-space: nowrap;
	display: inline-block;
}

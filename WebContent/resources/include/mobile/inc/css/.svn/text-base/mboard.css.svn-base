@charset "utf-8";
/* CSS Document */
.bd00 {
	table-layout: fixed;
	border-collapse: separate;
	border-spacing: 0px;
	border-top: 2px solid #449103;
	border-left: 1px solid #dadde1;
	margin: 5px 0 15px 0;
	background: #fff
}
.bd00 th {
	background: #eefde0;
	border-bottom: 1px solid #dadde1;
	border-right: 1px solid #dadde1;
	color: #306801;
	height: 31px;
}
.bd00 td {
	border-bottom: 1px solid #dadde1;
	border-right: 1px solid #dadde1;
	text-align: center;
	height: 31px;
}
.bd00 td.ta_l {
	text-align: left;
	padding-left: 10px;
}
.bd00 tr.btm {
	background: #f2f2f2;
}
.bd00 span.listx {
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-ms-text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}
.bd00 span.listc {
	width: 100%;
	overflow: hidden;
	text-overflow: clip;
	-o-text-overflow: clip;
	-ms-text-overflow: clip;
	white-space: nowrap;
	display: inline-block;
}
.bd00 input.mwd100 {
	width: 140px;
	max-width: 140px
}
.bd00 input.mwd55 {
	width: 45px;
	max-width: 45px
}
.bd00a {
	table-layout: fixed;
	border-collapse: separate;
	border-spacing: 0px;
	border-top: 2px solid #449103;
	border-left: 1px solid #dadde1;
	margin: 10px 0;
}
.bd00a th {
	background: #eefde0;
	border-bottom: 1px solid #dadde1;
	border-right: 1px solid #dadde1;
	color: #666;
	height: 28px;
}
.bd00a td {
	border-bottom: 1px solid #dadde1;
	border-right: 1px solid #dadde1;
	text-align: center;
}
.bd00view {
	border-top: 1px solid #dadde1;
	border-collapse: separate;
	border-spacing: 0px;
	background: #fff;
}
.bd00view th.bd01th {
	background: #eefde0;
	border-left: 2px solid #449103;
	border-right: 1px solid #dadde1;
	border-bottom: 1px solid #dadde1;
	text-align: center;
	color: #306801;
}
.bd00view td.bd01td {
	border-bottom: 1px solid #dadde1;
	border-right: 1px solid #dadde1;
	text-align: left;
	padding-left: 10px;
}
.bd00view td.listx {
	border-bottom: 1px solid #dadde1;
	text-align: left;
	padding-left: 10px;
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	-o-text-overflow: ellipsis;
	-ms-text-overflow: ellipsis;
	white-space: nowrap;
	display: inline-block;
}
.bd00view td.bd01tdC {
	height: auto;
	min-height: 150px;
	border-bottom: 1px solid #dadde1;
	padding: 10px;
}
.bd00view td.bd02tdC {
	background: #eefde0;
	height: auto;
	min-height: 150px;
	border: 1px solid #dadde1;
	border-top: 2px solid #449103;
	padding: 10px;
}
.bd00view th.bd03tdC {
	background: #eefde0;
	border: 1px solid #dadde1;
	border-top: 2px solid #449103;
	text-align: center
}
.bd00view td.bd03tdC {
	height: auto;
	border: 1px solid #dadde1;
	border-top: 0 none;
	padding: 0 10px 10px 10px;
}
.bd00view td.bd03tdL {
	height: auto;
	border-left: 1px solid #dadde1;
	padding: 10px 10px 0 10px;
}
.bd00view td.bd03tdR {
	height: auto;
	border-right: 1px solid #dadde1;
	padding: 10px 10px 0 10px;
	padding-left: 160px
}
.bd00view td.bd02tdC p.title_reply {
	font-weight: bold;
	width: 100%;
	overflow: hidden;
	margin-bottom: 20px
}
.bd00view td.bd01tdC p.photo img {
	width: 100%
}
.bd00view td.bd02tdCxtmp {
	padding: 0;
	border: 0 none;
	border-top: 2px solid #449103;
}
/*table type01*/

/*subRollBan Area*/
#subRollBan {
	background:url(../image/common/bg_subMainBan.gif) 0 0 no-repeat;
	position:relative;
	padding:6px 10px 12px 10px; 
	margin-bottom:20px;
}
#subRollBan .popzoneArea {
	position: relative;
	margin-top: 0px;
}
#subRollBan .controlArea {
	position: absolute;
	right: 0;
	top: 3px;
}
#subRollBan .controlArea li {
	float: left;
	margin-left: 2px;
}
#subRollBan a.pager-link {
	background:url(/resources/admin_include/commonfile/image/main/bg_no_off.gif) 0 0 no-repeat;
	font-size: 11px;
	color: #fff;
	width: 16px;
	height: 16px;
	display: inline-block;
	text-align: center;
	text-decoration: none;
	line-height: 1.5;
	margin-right: 3px; 
	font-weight:bold;
}
#subRollBan  a.pager-active {
	background: url(/resources/admin_include/commonfile/image/main/bg_no_on.gif) 0 0 no-repeat;
	font-size: 11px;
	color: #fff;
	width: 16px;
	height: 16px;
	display: inline-block;
	text-align: center;
	text-decoration: none;
	line-height: 1.5; 
	font-weight:bold;
}
#subRollBan  a.pager-link:link {
color#fff;
}
#subRollBan  .bx-pager {
	position: absolute;
	top: 3px;
	left: 5px;
}
#subRollBan  .popView li {
	margin-top: 25px;
	text-align: center;
	width: 720px;
	height: 170x;
}
#subRollBan  .popView li img {
	width: 720px;
	height: 170x;
}



.txtBtn_Gray {
	background: url(/resources/admin_include/commonfile/image/board/bg_btn_gray.png) 0 0 repeat-x;
	text-align: center;
	position: relative;
	display: inline-block;
	height: 20px;
	border: 1px solid #666;
}
.txtBtn_Gray a {
	color: #fff;
	text-decoration: none;
	display: inline-block;
	padding: 0 15px;
	padding-top: 3px;
 *padding-top:0;
	font-weight: bold;
	text-align: center;
	line-height: 1.6
}
.inputBtn_Gray {
	background: url(/resources/admin_include/commonfile/image/board/bg_btn_gray.png) 0 0 repeat-x;
	text-align: center;
	position: relative;
	display: inline-block;
	height: 22px;
	border: 1px solid #666;
	color: #fff;
	padding: 0 15px;
	padding-top: 2px;
 *padding-top:0;
	font-weight: bold;
	display: inline-block;
}
.txtBtn_lightGray {
	background: url(/resources/admin_include/commonfile/image/board/bg_btn_lightgray.png) 0 0 repeat-x;
	text-align: center;
	position: relative;
	display: inline-block;
	height: 20px;
	border: 1px solid #ccc;
}
.txtBtn_lightGray a {
	color: #262626;
	text-decoration: none;
	display: inline-block;
	padding: 0 15px;
	padding-top: 3px;
 *padding-top:0;
	font-weight: bold;
	text-align: center;
	line-height: 1.6
}


/*contents deco*/
.pageSection {
	margin-top: 20px;
}
/*table*/
.tblWrap {
	background: none;
	padding:0;
	margin:0
}
.tbl01 {
	table-layout: fixed;
	border-collapse: separate;
	empty-cells: show;
	width:100%;
	border-spacing:0;
	border-top: 1px solid #d7d7d7;
	border-left: 1px solid #d7d7d7;
}
.tbl01 th {
	padding: 7px;
	background: #f5f5f5;
	text-align: center;
	border-bottom: 1px solid #d7d7d7;
	border-right: 1px solid #d7d7d7;
}
.tbl01 td {
	padding: 7px;
	border-bottom: 1px solid #d7d7d7;
	border-right: 1px solid #d7d7d7;
}
.tdCenter td { text-align:center !important}
.progThumb IMG { width:65px  !important; height:65px  !important; }



.ttabWrap{ background:url(../image/common/bg_dotline.png) 0 bottom repeat-x; padding-bottom:1px; clear:both; overflow: hidden; overflow:hidden}

#ttab3 { background:url(/resources/admin_include/commonfile/image/common/bg_tap.png) 0 0 no-repeat; overflow:hidden; padding-top:5px}
#ttab3 li {
	float: left;
	display: inline-block;
	padding-left: 2px;
	font-weight: bold;
	text-align: center;
	/*background: url(/resources/admin_include/commonfile/image/admin/tab_body_off.png) repeat-x 0 2px;*/
	position: relative;
	margin-left: 5px;
	overflow: hidden;
	font-family: 'NanumGothic';
}
#ttab3 li:first-child { margin-left:20px}
#ttab3 li a {
	text-decoration: none;
	display: inline-block;
	color:#fff;
	height: 23px;
	padding: 10px 15px 0 3px;
	font-family: NanumGothic;
	font-size: 11px;
	min-width:30px;
	/*background: url(/resources/admin_include/commonfile/image/admin/tab_a_off.png) right bottom no-repeat*/
	font-weight:bold;
}
#ttab3 li.long a{
	font-size:12px !important;
}
#ttab3 li.dbl a{
	padding: 5px 5px 0 5px !important;
	height: 47px !important;
	line-height:1.2em !important;
}
#ttab3 li.on, #ttab3 li.choiced {
	background: url(/resources/admin_include/commonfile/image/common/tab_body_on.png) no-repeat 0 3px;
	color: #5b5b5b;
}
#ttab3 li.on a, #ttab3 li.choiced a {
	color: #5b5b5b;
	text-decoration: none;
	background: url(/resources/admin_include/commonfile/image/common/tab_a_on.png) right bottom no-repeat

}
#ttab4 { overflow:hidden}

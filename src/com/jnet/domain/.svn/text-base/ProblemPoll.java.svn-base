package com.jnet.domain;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class ProblemPoll extends Poll{
	private static final long serialVersionUID = 1L;
	/**
	 * 질문번호
	 */
	private int problemSeq;
	private int[] problemSeqArr;
	
	/**
	 * 부모번호
	 */
	private int prntProblemSeq;
	
	/**
	 * 질문
	 */
	private String problemContent;
	private String[] problemContentArr;
	
	private String[] questionCntArr;
	/**
	 * 질문 타입 
	 * 0 : 객관식
	 * 1 : 주관식
	 */
	private String problemType;
	private String[] problemTypeArr;
	
	public int[] getProblemSeqArr() {
		return problemSeqArr;
	}



	public void setProblemSeqArr(int[] problemSeqArr) {
		this.problemSeqArr = problemSeqArr;
	}



	public int getPrntProblemSeq() {
		return prntProblemSeq;
	}



	public void setPrntProblemSeq(int prntProblemSeq) {
		this.prntProblemSeq = prntProblemSeq;
	}



	public String[] getQuestionCntArr() {
		return questionCntArr;
	}



	public void setQuestionCntArr(String[] questionCntArr) {
		this.questionCntArr = questionCntArr;
	}



	public int getProblemSeq() {
		return problemSeq;
	}



	public void setProblemSeq(int problemSeq) {
		this.problemSeq = problemSeq;
	}



	public String getProblemContent() {
		return problemContent;
	}



	public void setProblemContent(String problemContent) {
		this.problemContent = problemContent;
	}



	public String[] getProblemContentArr() {
		return problemContentArr;
	}



	public void setProblemContentArr(String[] problemContentArr) {
		this.problemContentArr = problemContentArr;
	}



	public String getProblemType() {
		return problemType;
	}



	public void setProblemType(String problemType) {
		this.problemType = problemType;
	}



	public String[] getProblemTypeArr() {
		return problemTypeArr;
	}



	public void setProblemTypeArr(String[] problemTypeArr) {
		this.problemTypeArr = problemTypeArr;
	}



	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
}

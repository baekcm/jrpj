package com.jnet.domain;

import org.apache.commons.lang.builder.ToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class Poll extends Cts{
	private static final long serialVersionUID = 1L;
	/**
	 * 설문번호
	 */
	private int pollSeq;
	
	/**
	 * 설문제목
	 */
	private String subject;
	
	/**
	 * 설문 시작날짜
	 */
	private String stDtm;
	
	/**
	 * 설문 종료날짜
	 */
	private String edDtm;
	
	/**
	 * 삭제유무
	 */
	private String rmYn;
	
	/**
	 * 투표수
	 */
	private Integer answerCnt;
	
	/**
	 * 투표상황
	 */
	private String pollStatus;
	/**
	 * 투표상황코드
	 * R : 대기중
	 * I : 진행중
	 * C : 종료
	 */
	private String pollStatusCd;
	
	/**
	 * 내 설문조사 했는지 여부
	 */
	private int myPollCnt;
	
	/**
	 * 임시저장 
	 */
	private String useYn;
	
	public String getUseYn() {
		return useYn;
	}



	public void setUseYn(String useYn) {
		this.useYn = useYn;
	}



	public String getPollStatusCd() {
		return pollStatusCd;
	}



	public void setPollStatusCd(String pollStatusCd) {
		this.pollStatusCd = pollStatusCd;
	}



	public int getMyPollCnt() {
		return myPollCnt;
	}



	public void setMyPollCnt(int myPollCnt) {
		this.myPollCnt = myPollCnt;
	}



	public String getPollStatus() {
		return pollStatus;
	}



	public void setPollStatus(String pollStatus) {
		this.pollStatus = pollStatus;
	}



	public Integer getAnswerCnt() {
		return answerCnt;
	}



	public void setAnswerCnt(Integer answerCnt) {
		this.answerCnt = answerCnt;
	}



	public int getPollSeq() {
		return pollSeq;
	}



	public void setPollSeq(int pollSeq) {
		this.pollSeq = pollSeq;
	}



	public String getSubject() {
		return subject;
	}



	public void setSubject(String subject) {
		this.subject = subject;
	}



	public String getStDtm() {
		return stDtm;
	}



	public void setStDtm(String stDtm) {
		this.stDtm = stDtm;
	}



	public String getEdDtm() {
		return edDtm;
	}



	public void setEdDtm(String edDtm) {
		this.edDtm = edDtm;
	}



	public String getRmYn() {
		return rmYn;
	}



	public void setRmYn(String rmYn) {
		this.rmYn = rmYn;
	}



	public String toString() {
		return ToStringBuilder.reflectionToString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
}
